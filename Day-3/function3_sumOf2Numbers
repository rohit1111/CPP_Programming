#include <iostream>
using namespace std;

int sumOfTwoNumbers(int num1, int num2);

int main(){
	int number1, number2;
	cin>>number1>>number2;

	int result = sumOfTwoNumbers(number1,number2);
	cout<<"Number1: "<<number1<<", Number2: "<<number2<<", Sum: "<<result<<endl;
	return 0;
}

int sumOfTwoNumbers(int num1, int num2){
	int sum = 0;
	sum = num1 + num2;
	return sum;
}
